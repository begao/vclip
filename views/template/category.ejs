<div class="category-page">
<%- partial ('common/navbar.ejs') %>
<div class="container-fluid">
  <div id="list-post" class="col-sm-8">
    <div class="panel panel-default">
      <div class="panel-heading"><i class="fa fa-film"></i> <%= foundCategory.name %></div>
      <div class="panel-body">
        <% _.each(foundCategory.posts, function(post) { %>
        <a href="/post/view?id=<%= post.id %>&slug=<%= post.slug %>">
          <div class="col-md-3 col-xs-3 col-sm-3 new-post">
            <img class="thumb-post" src="<%= post.thumbnail %>">
            <!--Thumbnail 270x400-->
            <div class="name-post">
              <span class="post-title"><%= post.name %></span>
              <span class="desc-post"><i class="fa fa-clock-o"></i> <%= post.time %></span>
            </div>
          </div>
        </a>
        <% }) %>
      </div>
      <div class="panel-footer text-center">
        <ul class="pagination">
          <li><a href="/category/view?id=<%= foundCategory.id %>&page=1">1</a></li>
          <li class="active"><a href="#">2</a></li>
          <li><a href="#">3</a></li>
          <li><a href="#">4</a></li>
          <li><a href="#">5</a></li>
        </ul>
      </div>
    </div>
  </div>

  <%- partial ('common/column-right.ejs') %>

</div>
</div>
